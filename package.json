{
  "name": "eslint-plugin-emmanuel",
  "version": "0.1.2",
  "description": "My ESLint plugin",
  "repository": "https://github.com/Manu1400/eslint-plugin-emmanuel",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Emmanuel Haguet <emmanuel.haguet@laposte.net>",
  "main": "index.js",
  "scripts": {
    "test": "mocha tests/lib --recursive",
    "test:config": "eslint tests/essai.js",
    "lint": "eslint ./tests/ ./lib/ && eslint --ext md .",
    "fix": "eslint --fix-dry-run && eslint --fix",
    "docs": "./node_modules/eslint-docs/dist/cli.js check",
    "perf": "TIMING=1 eslint test/**",
    "////": "dont work:",
    "perf:export": "eslint tests --rule \"timeout: 2\" --quiet -o=newfile.md > ruletable.md",
    "integration": "./tests/integration/test.js"
  },
  "dependencies": {
    "execa": "^1.0.0",
    "import-modules": "^1.1.0",
    "is-url": "^1.2.4",
    "listr": "^0.14.3",
    "path": "^0.12.7",
    "reflect-type-3": "^1.0.0",
    "requireindex": "~1.1.0",
    "tempy": "^0.2.1",
    "xpath.js": "^1.1.0"
  },
  "devDependencies": {
    "@getify/eslint-plugin-proper-ternary": "^3.0.1",
    "@manu1400/eslint-plugin-jsonschema": "^0.3.2",
    "babel-eslint": "^10.0.1",
    "eslint-docs": "^0.4.0",
    "eslint-plugin-arca": "^0.8.1",
    "eslint-plugin-eslint-plugin": "^2.0.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-no-hyogo-police": "^1.0.0",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-self": "^1.2.0",
    "import-lazy": "^3.1.0",
    "mocha": "^6.0.2"
  },
  "peerDependencies": {
    "eslint": ">=5.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "license": "ISC"
}
